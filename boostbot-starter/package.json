{
  "name": "boostbot-starter",
  "version": "1.0.0",
  "description": "A simple Helipad webhook integration for posting boosts to Nostr",
  "main": "helipad-webhook.js",
  "type": "module",
  "scripts": {
    "start": "node helipad-webhook.js",
    "dev": "tsx --watch helipad-webhook.js",
    "build": "tsc",
    "test": "node test-webhook.js",
    "docker:build": "docker build -t boostbot-starter .",
    "docker:run": "docker compose up -d",
    "docker:stop": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "docker:restart": "docker compose restart",
    "docker:status": "docker compose ps"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.20.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "nostr-tools": "^2.7.0",
    "ws": "^8.18.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.19.4",
    "@types/ws": "^8.5.12",
    "tsx": "^4.16.2",
    "typescript": "^5.8.3"
  }
} 
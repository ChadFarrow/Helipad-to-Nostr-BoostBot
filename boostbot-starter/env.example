# BoostBot Starter - Environment Variables
# Copy this file to .env and edit with your values

# REQUIRED: Your Nostr private key (nsec format)
# Generate one at https://nostr-keygen.com/
NOSTR_BOOST_BOT_NSEC=nsec1your_actual_nsec_here

# OPTIONAL: Webhook authentication token
# If set, Helipad must include: Authorization: Bearer YOUR_TOKEN
# Leave empty to disable authentication
HELIPAD_WEBHOOK_TOKEN=

# OPTIONAL: Test mode (set to 'true' to log without posting to Nostr)
TEST_MODE=false

# OPTIONAL: Server port (default: 3333)
PORT=3333

# Log level (ERROR, WARN, INFO, DEBUG)
LOG_LEVEL=INFO 
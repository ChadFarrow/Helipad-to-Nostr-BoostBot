# Helipad BoostBot Configuration
# Copy this file to .env and fill in your values

# Required: Your Nostr private key for the boost bot
# Generate with: noscl key-gen
NOSTR_BOOST_BOT_NSEC=your_nostr_private_key_here

# Required: Helipad webhook authentication token
# This should match the token configured in your Helipad webhook settings
HELIPAD_WEBHOOK_TOKEN=your_webhook_token_here

# Optional: Port to run on (default: 3333)
PORT=3333

# Optional: Test mode (set to true to log without posting to Nostr)
TEST_MODE=false

# Optional: Node environment
NODE_ENV=production 